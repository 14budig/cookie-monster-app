<div id="cookies_index_page">
  <p id="notice"><%= notice %></p>

  <h1>Hack the Cookies!</h1>
    <%= link_to "Go to your Favorite Cookie", favorite_cookie_path %>


  <ul>
    <% @cookies.each do |cookie| %>
      <li>
        <%= link_to "Show Cookie #{cookie.id}", show_cookie_path(cookie.id) %>
      </li>
    <% end %>
  </ul>
</div>
